{
  "name": "simple-react-boilerplate",
  "version": "1.0.0",
  "description": "Initially built for chrome extensions",
  "scripts": {
    "clean": "rimraf app/dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "watch": "webpack --config webpack.config.dev.js --watch",
    "start": "npm run clean && npm run watch",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nadimtuhin/simple-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "webpack",
    "babel"
  ],
  "author": "Nadim Tuhin <nadimtuhin@gmail.com> (http://github.com/nadimtuhin)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/nadimtuhin/simple-react-boilerplate/issues"
  },
  "homepage": "https://github.com/nadimtuhin/simple-react-boilerplate",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
